# Environment Variables for Error Tracking
# Copy this to .env and fill in your actual values

# Sentry Configuration (Optional - leave empty to disable error tracking)
VITE_SENTRY_DSN=https://your-dsn@sentry.io/project-id
VITE_APP_VERSION=1.0.0

# Error Tracking Settings
VITE_ERROR_TRACKING_ENABLED=true
VITE_PERFORMANCE_MONITORING=true

# Development Settings
VITE_LOG_LEVEL=debug

# Production Settings (use in .env.production)
# VITE_LOG_LEVEL=error
# VITE_ERROR_TRACKING_ENABLED=true

# Appwrite Configuration (if needed)
# VITE_APPWRITE_ENDPOINT=https://fra.cloud.appwrite.io/v1
# VITE_APPWRITE_PROJECT_ID=your_project_id
# VITE_DATABASE_ID=your_database_id
# VITE_TASKS_COLLECTION_ID=your_tasks_collection_id
# VITE_BUCKET_ID=your_bucket_id
